query{
    deliveryOptions(
        checkoutId: "{checkoutUUID}",
        query: {
            coordinates:{
                longitude: "1.3121980",
                latitude: "51.1264930"
            }
        }
    )
    {
        id
        optionId
        type
        price{
            amount
            currency
        }
        cutoff
        timeWindow{
            from
            to
        }
    }
}